{
  "compilerOptions": {
    "allowJs": true,
    "target": "ES2020",
    "module": "ES2020",
    "lib": ["es2020", "DOM", "DOM.Iterable"],
    "jsx": "react-jsx",
    "jsxImportSource": "@builder.io/qwik",
    "strict": true,
    "declaration": true,
    "declarationDir": "lib-types",
    "resolveJsonModule": true,
    "moduleResolution": "Bundler",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "incremental": true,
    "isolatedModules": true,
    "types": ["vite/client"],
    "paths": {
      "~/*": ["./src/*"]
    }
  },
  "include": ["src", "./*.d.ts"]
}
